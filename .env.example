# ============================================================================================
# .env.example - Cloelia AI Agent System Environment Configuration
# Author: Khaylub Thompson-Calvin | Date: 2025-05-10
# ============================================================================================

# ========================
# 🌐 API KEYS - External Services
# ========================
OPENAI_KEY=your-openai-api-key-here
ELEVENLABS_KEY=your-elevenlabs-api-key-here
CLOELIA_API_KEY=your-cloelia-api-key-here
TELEGRAM_BOT_TOKEN=your-telegram-bot-token-here
TWILIO_ACCOUNT_SID=your-twilio-account-sid-here
TWILIO_AUTH_TOKEN=your-twilio-auth-token-here

# ========================
# 🛡️ Security & Encryption
# ========================
ALGORITHM_KEY=your-algorithm-secret-key
SECRET_KEY=your-flask-or-fastapi-secret-key
JWT_SECRET_KEY=your-jwt-secret-key
JWT_ALGORITHM=HS256

# ========================
# 🗄️ Database Configuration (PostgreSQL)
# ========================
DB_NAME=cloeila_dev
DB_USER=postgres
DB_PASSWORD=your-db-password
DB_HOST=localhost
DB_PORT=8888
DATABASE_URL=postgresql://postgres:your-db-password@localhost:8888/cloeila_dev

# ========================
# 📦 Class & Legacy Vault Settings
# ========================
ENABLE_CLASS_UNLOCK=true
DEFAULT_ROLE_TYPE=Seeker

# ========================
# 🎙️ Audio and Voice Settings
# ========================
DEFAULT_VOICE_PROFILE=Default
PREFERRED_LANGUAGE=en

# ========================
# ⚙️ App Settings
# ========================
APP_ENV=development
APP_DEBUG=True
APP_PORT=8000
LOG_LEVEL=INFO

# ========================
# 📡 Vector & Queue Systems
# ========================
VECTOR_DB_URL=localhost:8000
REDIS_URL=localhost:6379



